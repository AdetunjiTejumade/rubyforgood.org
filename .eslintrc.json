{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "curly": ["error", "all"],
      "no-tabs": ["error", {"allowIndentationTabs": true}]
    }
}
